{
  "info": {
    "title": "Bitcoin Address Generation API",
    "description": "An API to generate mnemonic seeds and various types of Bitcoin addresses.",
    "version": "1.0.0"
  },
  "openapi": "3.1.0",
  "components": {
    "schemas": {
      "MnemonicResponse": {
        "type": "object",
        "properties": {
          "BIP39Mnemonic": {
            "type": "string"
          },
          "BIP39Seed": {
            "type": "string"
          }
        }
      },
      "AddressRequest": {
        "properties": {
          "mnemonic": {
            "type": "string",
            "title": "Mnemonic"
          },
          "num_addresses": {
            "anyOf": [
              {
                "type": "integer"
              },
              {
                "type": "null"
              }
            ],
            "title": "Num Addresses",
            "default": 5
          }
        },
        "type": "object",
        "required": [
          "mnemonic"
        ],
        "title": "AddressRequest"
      },
      "AddressDetails": {
        "properties": {
          "address": {
            "type": "string",
            "title": "Address"
          },
          "private_key": {
            "type": "string",
            "title": "Private Key"
          },
          "public_key": {
            "type": "string",
            "title": "Public Key"
          },
          "derivation_path": {
            "type": "string",
            "title": "Derivation Path"
          }
        },
        "type": "object",
        "required": [
          "address",
          "private_key",
          "public_key",
          "derivation_path"
        ],
        "title": "AddressDetails"
      },
      "AddressListResponse": {
        "properties": {
          "addresses": {
            "items": {
              "$ref": "#/components/schemas/AddressDetails"
            },
            "type": "array",
            "title": "Addresses"
          }
        },
        "type": "object",
        "required": [
          "addresses"
        ],
        "title": "AddressListResponse"
      },
      "BrainWalletRequest": {
        "type": "object",
        "properties": {
          "passphrase": {
            "type": "string"
          }
        }
      },
      "BrainWalletResponse": {
        "type": "object",
        "properties": {
          "wif_private_key": {
            "type": "string"
          },
          "bitcoin_address": {
            "type": "string"
          },
          "public_key": {
            "type": "string"
          }
        }
      },
      "HTTPValidationError": {
        "properties": {
          "detail": {
            "items": {
              "$ref": "#/components/schemas/ValidationError"
            },
            "type": "array",
            "title": "Detail"
          }
        },
        "type": "object",
        "title": "HTTPValidationError"
      },
      "ValidationError": {
        "properties": {
          "loc": {
            "items": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "type": "integer"
                }
              ]
            },
            "type": "array",
            "title": "Location"
          },
          "msg": {
            "type": "string",
            "title": "Message"
          },
          "type": {
            "type": "string",
            "title": "Error Type"
          }
        },
        "type": "object",
        "required": [
          "loc",
          "msg",
          "type"
        ],
        "title": "ValidationError"
      }
    }
  },
  "servers": [
    {
      "url": "http://127.0.0.1:8000",
      "description": "Development server"
    },
    {
      "url": "https://btc-tx-gw-yanncarliers-projects.vercel.app/",
      "description": "Production environment"
    }
